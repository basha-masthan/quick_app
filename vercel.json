{
  "version": 2,
  "builds": [
    {
      "src": "vercel_app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "vercel_app.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "project.settings"
  },
  "buildCommand": "python manage.py collectstatic --noinput"
}